@model List<(string email, bool isSubscribed)>

<div class="p-4">
    <a type="button" class="btn btn-primary mb-4" asp-controller="SendNews" asp-action="SendMail">Send mail</a>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Email</th>
                <th scope="col">Is subscribed</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.email</td>
                    <td>@item.isSubscribed</td>
                </tr>
            }
        </tbody>
    </table>
</div>